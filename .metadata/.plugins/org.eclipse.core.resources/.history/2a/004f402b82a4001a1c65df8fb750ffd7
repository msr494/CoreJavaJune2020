package javBasicsShamim;

public class HelloWorld {

	public static void main(String[] args) {
//will practice multiple line print
		System.out.println("this is my first project");
		System.out.print("i am practicing next line print");
		System.out.println("2 lines will print together");
		
		System.out.println("this is my first project");
		System.out.println("i am practicing next line print");
		System.out.println("2 lines will print in separate lines");
		
		System.out.println("this is my first project");
		System.out.print("i am practicing next line print");
		System.out.println("\r2 lines will print separate using carriage return");
		
		System.out.println("this is my first project");
		System.out.print("i am practicing next line print");
		System.out.println("\n2 lines will print separate");
		
		System.out.println("i am, testing \tto create space");
		
		
		
	}

}
